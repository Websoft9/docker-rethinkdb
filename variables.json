{
  "name": "rethinkdb",
  "trademark": "RethinkDB",
  "installpath": "/data/wwwroot/rethinkdb",
  "compose_file": "docker-compose.yml",
  "fork_url": "https://github.com/rethinkdb/rethinkdb-dockerfiles",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "1",
    "memory": "1",
    "disk": "20",
    "swap": "2",
    "url": "https://www.rethinkdb.org/docs/user_guide/en/install-requirements.html"
  },

  "services":[
    {
      "name": "RethinkDB",
      "port": "9091",
      "description_cn": "浏览器访问 RethinkDB",
      "description_en": "Browser access to RethinkDB by http",
      "required": "Y"
    }
  ]

}